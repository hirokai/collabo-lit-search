{
  "manifest_version": 2,

  "name": "Collabo lit search",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "http://localhost:3000/",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": ["jquery.min.js","lodash.min.js", "pubnub.min.js", "eventPage.js"],
    "persistent": false
  }
}
